%p#notice= notice
%h1 Feeds
%table
  %thead
    %tr
      %th Title
      %th Slug
      %th Body
      %th Type
      %th{colspan: "3"}
  %tbody
    - @feeds.each do |feed|
      %tr
        %td= feed.title
        %td= feed.slug
        %td= feed.body
        %td= feed.type
        %td= link_to 'Show', feed
        %td= link_to 'Edit', edit_feed_path(feed)
        %td= link_to 'Destroy', feed, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Feed', new_feed_path
