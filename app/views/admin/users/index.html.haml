%ol.breadcrumb

%h3 Users
%table.table
  %thead
    %tr
      %th First Name
      %th Last Name
      %th Email
      %th Actions
  %tbody
    - @users.each do |user|
      %tr
        %td= user.first_name
        %td= user.last_name
        %td= user.email
        %td
          = link_to 'Details', admin_user_path(user)
